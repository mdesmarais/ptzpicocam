<?xml version="1.0"?>
<robot name="wall_apside">
    <link name="world" />
    <link name="wall_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual>
            <geometry>
                <box size="1.0 1.0 1.0"/>
            </geometry>
            <material name="body_material">
				<texture filename="apside.jpeg"/>
                
            </material>
        </visual>
    </link>

    <joint name="world_joint" type="fixed">
        <parent link="world"/>
        <child link="wall_link"/>
    </joint>
</robot>